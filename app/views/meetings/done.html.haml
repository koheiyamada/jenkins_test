=content_for :tabmenu do
  =render 'nav_tab'

%h1=title t('meeting.title.done')

=render 'menu'

-if @meetings.empty?
  =t('common.not_found')
-else
  =render layout: 'meetings', locals: {meetings: @meetings} do |meeting|
    =link_button_to 'zoom-in', t('common.detail'), {action: :show, id: meeting}
  =paginate @meetings
