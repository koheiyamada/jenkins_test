-title t('lesson.titles.not_started')
=content_for :tabmenu do
  =render 'nav_tab'
=render "pa/student_header", student:@student

=render 'menu'

-if @lessons.blank?
  =t('common.not_found')
-else
  =render layout: 'lessons', locals: {lessons: @lessons} do |lesson|
    =img_link_to 'common.detail', {action: 'show', id: lesson}, class: 'small'
  =paginate @lessons
