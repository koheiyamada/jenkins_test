%h1=title t("titles.basic_lesson_reservation")

-if @students.blank?
  =t('common.not_found')
-else
  =render layout:"students/students", locals:{students:@students} do |student|
    .action
      =img_link_to "common.select", new_bs_student_basic_lesson_info_path(student), class:"small"
  =paginate @students